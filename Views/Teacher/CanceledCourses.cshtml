@model IEnumerable<SchoolSystem.Models.Course>

<div class="container mx-auto my-6">
    <h2 class="text-2xl font-bold mb-4">Canceled Courses</h2>
    <table class="w-full text-center border-collapse border border-gray-300">
        <thead>
            <tr class="bg-gray-200">
                <th class="border border-gray-300 px-4 py-2">Course Code</th>
                <th class="border border-gray-300 px-4 py-2">Course Name</th>
                <th class="border border-gray-300 px-4 py-2">Units</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var course in Model)
            {
                <tr>
                    <td class="border border-gray-300 px-4 py-2">@course.CourseCode</td>
                    <td class="border border-gray-300 px-4 py-2">@course.CourseName</td>
                    <td class="border border-gray-300 px-4 py-2">@course.Units</td>
                </tr>
            }
        </tbody>
    </table>
</div>
